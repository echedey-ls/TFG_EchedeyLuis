@inproceedings{Holmgren_Groenendyk_2016,
  title        = {An open source solar power forecasting tool using PVLIB-Python},
  url          = {https://ieeexplore.ieee.org/document/7749755},
  doi          = {10.1109/PVSC.2016.7749755},
  abstractnote = {We describe an open-source PV power forecasting tool based on the PVLIB-Python library. The tool allows users to easily retrieve standardized weather forecast data relevant to PV power modeling from NOAA/NCEP/NWS models including the GFS, NAM, RAP, HRRR, and the NDFD. A PV power forecast can then be obtained using the weather data as inputs to the comprehensive modeling capabilities of PVLIB-Python. Standardized, open source, reference implementations of forecast methods using publicly available data may help advance the state-of-the-art of solar power forecasting.},
  booktitle    = {2016 IEEE 43rd Photovoltaic Specialists Conference (PVSC)},
  author       = {Holmgren, William F. and Groenendyk, Derek G.},
  year         = {2016},
  month        = jun,
  pages        = {0972–0975}
}

