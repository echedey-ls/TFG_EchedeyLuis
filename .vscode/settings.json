{
    "latex-workshop.latex.build.forceRecipeUsage": false,
    "latex-workshop.latex.jobname": "TFG_Luis_Alvarez_Echedey",
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-jobname=%OUTDIR%/%DOCFILE%",
                "%DOC%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "-jobname=%DOCFILE%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%OUTDIR%/%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%OUTDIR%/%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "pdflatexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-outdir=%OUTDIR%",
                "-jobname=%DOCFILE%",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex>biber>makeglossaries>pdflatex*2",
            "tools": [
                // Use pdflatex, biber, and makeglossaries
                "pdflatex",
                "biber",
                "makeglossaries",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
    ],
